{
    "name": "dazzle-wordpress/http",
    "type": "library",
    "description": "Dazzle HTTP Package",
    "keywords": ["dazzle", "wordpress", "http"],
    "homepage": "https://github.com/dazzle-wordpress/http",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "dazzle-wordpress/uri": "^1.0|^2.0",
        "composer/ca-bundle": "^1.0",
        "laminas/laminas-diactoros": "^2.2.2",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "ext-curl": "To use cURL for HTTP connections"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Http\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
